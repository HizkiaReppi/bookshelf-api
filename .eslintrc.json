{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "import/extensions": [
            "error",
            "always"
        ]
    },
    "ignorePatterns": [
        "node_modules/"
    ]
}